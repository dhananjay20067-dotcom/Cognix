#2
def factorial(n):
    result = 1
    for i in range(1, n + 1):
        result = result * i
    return result

#3
def count_vowels(zomato):
    vowels = "aeiouAEIOU"
    count = 0
    for char in zomato: # Iterate through each character in the 'zomato' string
        if char in vowels: # Check if the current character is a vowel
            count += 1
    return count

#5
def reverse_string(text):
    return text[::-1]



    
    
        

